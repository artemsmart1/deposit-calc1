void check  (int cash,int day);

void check (int cash,int day){
	if (cash<10000 || day <0 || day>365){
	printf ("error ");
	}
	
}
int main (){

float cash=0; float day=0;
printf ("Vvedite summu vklada :");
scanf ("%f",&cash);
printf ("Vvedite kol-vo dney:");
scanf ("%f",&day);
check (day,cash);
return 0;
}
