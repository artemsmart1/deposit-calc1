#include <stdlib.h>
#include <stdio.h>

int main (){
float cash=0; float day=0;
printf ("Vvedite summu vklada :");
scanf ("%f",&cash);
if (cash<10000){
	printf ("error ");
	return 0;
}
printf ("Vvedite kol-vo dney:");
scanf ("%f",&day);
if (day>365 || day<0){
	printf ("error ");
	return 0;
}
if(cash<=100000 && cash>=10000 ) {
    if(day>0 && day<=30) {
      cash=cash-(cash*0.1);
    }  if(day>=31 && day<=120) {
        cash=cash+(cash*0.02);
    }  if(day>=121 && day<=240) {
        cash=cash+(cash*0.06);
    }  if(day>=241 && day<=365) {
        cash=cash+(cash*0.12);
    }